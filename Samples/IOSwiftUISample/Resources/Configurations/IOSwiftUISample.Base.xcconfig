ASSETCATALOG_COMPILER_APPICON_NAME = AppIcon
ASSETCATALOG_COMPILER_GLOBAL_ACCENT_COLOR_NAME = AccentColor
CODE_SIGN_ENTITLEMENTS = IOSwiftUISample/IOSwiftUISampleDebug.entitlements
CODE_SIGN_STYLE = Manual
DEVELOPMENT_ASSET_PATHS = "IOSwiftUISample/Preview Content"
ENABLE_PREVIEWS = YES
GENERATE_INFOPLIST_FILE = YES
INFOPLIST_FILE = IOSwiftUISample/Info.plist
LD_RUNPATH_SEARCH_PATHS = $(inherited) @executable_path/Frameworks
SWIFT_EMIT_LOC_STRINGS = YES

INFOPLIST_KEY_UILaunchScreen_Generation = YES
INFOPLIST_KEY_NSCameraUsageDescription = We need to access camera for adding a new photo.
INFOPLIST_KEY_NSFaceIDUsageDescription = We need Face ID permission for biometric authentication.
INFOPLIST_KEY_NSLocationAlwaysUsageDescription = We need your location for display on your profile page.
INFOPLIST_KEY_NSLocationAlwaysAndWhenInUseUsageDescription = We need your location for display on your profile page.
INFOPLIST_KEY_NSLocationUsageDescription = We need your location for display on your profile page.
INFOPLIST_KEY_NSLocationWhenInUseUsageDescription = We need your location for display on your profile page.
INFOPLIST_KEY_NFCReaderUsageDescription = We need NFC reader permission to perform document scanning.
INFOPLIST_KEY_UISupportedInterfaceOrientations_iPad = UIInterfaceOrientationPortrait UIInterfaceOrientationPortraitUpsideDown UIInterfaceOrientationLandscapeLeft UIInterfaceOrientationLandscapeRight
INFOPLIST_KEY_UISupportedInterfaceOrientations_iPhone = UIInterfaceOrientationPortrait
INFOPLIST_KEY_UIApplicationSupportsIndirectInputEvents = YES
INFOPLIST_KEY_UIUserInterfaceStyle = Light
